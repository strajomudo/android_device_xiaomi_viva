# Health
genfscon sysfs /devices/platform/1100f000.i2c3/i2c-3/3-0051/power_supply/ln8000-standalone   u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/11016000.i2c5/i2c-5/5-0051/power_supply/ln8000-standalone-2 u:object_r:sysfs_batteryinfo:s0
