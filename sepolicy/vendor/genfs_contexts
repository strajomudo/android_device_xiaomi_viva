genfscon sysfs /devices/platform/11015000.spi5/spi_master/spi5/spi5.0/clk_enable u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11015000.spi5/spi_master/spi5/spi5.0/irq u:object_r:sysfs_fingerprint:s0

# Graphics
genfscon sysfs /devices/platform/14000000.dispsys_config/drm/card0/card0-DSI-1/panel_event u:object_r:vendor_sysfs_graphics:s0

